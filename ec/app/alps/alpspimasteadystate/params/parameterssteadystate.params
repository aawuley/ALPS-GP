gp.fs.0.func.12    = ec.app.alpspimasteadystate.fs.Div
gp.fs.0.func.12.nc = nc2
gp.fs.0.func.13    = ec.app.alpspimasteadystate.fs.Max
gp.fs.0.func.13.nc = nc2
gp.fs.0.func.14    = ec.app.alpspimasteadystate.fs.Min
gp.fs.0.func.14.nc = nc2
gp.fs.0.func.15    = ec.app.alpspimasteadystate.fs.Ln
gp.fs.0.func.15.nc = nc1
gp.fs.0.func.16    = ec.app.alpspimasteadystate.fs.Log10
gp.fs.0.func.16.nc = nc1

eval.problem = ec.app.alpspimasteadystate.MultiValuedRegressionCoscA1B
eval.problem.data = ec.app.alpspimasteadystate.DoubleData
eval.problem.pima-indians-diabetes-data-raw        = ../Extensions/ecj22/ec/app/alpspimasteadystate/data/pima-indians-diabetes-data.txt
eval.problem.pima-indians-diabetes-data-clean      = ../Extensions/ecj22/ec/app/alpspimasteadystate/data/pima-indians-diabetes-data-new.txt
eval.problem.pima-indians-diabetes-data-train-data = ../Extensions/ecj22/ec/app/alpspimav/data/pima-indians-diabetes-train-data.txt
eval.problem.pima-indians-diabetes-data-test-data  = ../Extensions/ecj22/ec/app/alpspimasteadystate/data/pima-indians-diabetes-test-data.txt

eval.problem.total-number-of-points        = 168

#Specification of output statistics files

stat.num-children           = 2
stat.child.0                = ec.alps.SSShortStatistics
stat.child.0.file           = ../output/cmtrx.stats
stat.child.1                = ec.alps.SimpleStatistics
stat.child.1.silent.print   = true
stat.child.1.file           = ../output/ss.stats