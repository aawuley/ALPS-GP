gp.fs.0.func.2    = ec.app.alpspimasteadystate.ts.DiastolicBP
gp.fs.0.func.2.nc = nc0
gp.fs.0.func.3    = ec.app.alpspimasteadystate.ts.TricepsSkinFoldThickness
gp.fs.0.func.3.nc = nc0
gp.fs.0.func.4    = ec.app.alpspimasteadystate.ts.TwoHourSerumInsulin
gp.fs.0.func.4.nc = nc0
gp.fs.0.func.5    = ec.app.alpspimasteadystate.ts.BMI
gp.fs.0.func.5.nc = nc0
